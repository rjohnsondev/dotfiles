Config { font = "xft:Hack:size=12:antialias=true"
    , additionalFonts = []
    , borderColor = "black"
    , border = TopB
    , bgColor = "black"
    , fgColor = "grey"
    , alpha = 255
    , position = Bottom
    -- , position = Top
    , textOffset = -1
    , iconOffset = -1
    , lowerOnStart = True
    , pickBroadest = False
    , persistent = False
    , hideOnStart = False
    , iconRoot = "."
    , allDesktops = True
    , overrideRedirect = True
    , commands = [ Run Network "wlp58s0" ["-L","0","-H","32",
        "--normal","green","--high","red"] 10
            , Run Network "enx9cebe819f216" ["-L","0","-H","32",
        "--normal","green","--high","red"] 10
            , Run Network "tun0" ["-L","0","-H","32",
        "--normal","green","--high","red"] 10
            , Run MultiCpu ["-t" , "Cpu: <autototal>",
        "-L","3","-H","50",
        "--normal","green","--high","red"] 10
            , Run CoreTemp ["-t", "cpu: <core0>° core1: <core1>° core2: <core2>°",
        "-L", "40", "-H", "60",
        "-l", "lightblue", "-n", "gray90", "-h", "red"] 50
            , Run BatteryP ["BAT0"]
            ["-t", "<acstatus> <watts>W <left>%  <timeleft>",
        "-L", "10", "-H", "80", "-p", "3",
            "--", "-O", "<fc=green>On</fc> - ", "-i", "",
            "-L", "-15", "-H", "-5",
            "-l", "red", "-m", "blue", "-h", "green"] 600
                , Run Memory ["-t","Mem: <usedratio>%"] 10
                , Run Swap [] 10
                , Run Date "%a %Y-%m-%d %Y %H:%M:%S" "date" 10
                ]
                , sepChar = "%"
                , alignSep = "}{"
                , template = "%multicpu% | %memory% %swap% | %battery% | %coretemp% | %wlp58s0% %enx9cebe819f216% }\
                              \{ <fc=#ee9a00>%date%</fc>"
}
